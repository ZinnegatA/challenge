{
  "name": "challenge",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "test:e2e": "cross-env NODE_ENV=test jest --detectOpenHandles --runInBand --verbose --testPathPattern=test/e2e",
    "start:dev": "tsc && cross-env NODE_ENV=development nodemon src/app.ts",
    "start": "tsc && cross-env NODE_ENV=development npm run typeorm:migration:run && cross-env NODE_ENV=development NODE_PATH=./dist node dist/src/app.js",
    "typeorm:migration:generate": "tsc && cross-env NODE_ENV=development typeorm migration:generate src/migrations/Generated -d ./dist/orm.config.js",
    "typeorm:migration:run": "tsc && cross-env NODE_ENV=development typeorm migration:run -d ./dist/orm.config.js",
    "typeorm:migration:run:test": "tsc && cross-env NODE_ENV=test typeorm migration:run -d ./dist/orm.config.js",
    "typeorm:migration:revert": "tsc && cross-env NODE_ENV=development typeorm migration:revert -d ./dist/orm.config.js",
    "typeorm:migration:show": "tsc && cross-env NODE_ENV=test typeorm migration:show -d ./dist/orm.config.js",
    "build": "tsc",
    "lint": "cross-env NODE_ENV=development npx eslint ./src",
    "format": "cross-env NODE_ENV=development npx eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZinnegatA/challenge.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZinnegatA/challenge/issues"
  },
  "homepage": "https://github.com/ZinnegatA/challenge#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.3",
    "@types/passport": "^1.0.13",
    "@types/passport-jwt": "^3.0.10",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  }
}
